{
  "name": "nextjs-ssg",
  "version": "4.6.0",
  "private": true,
  "scripts": {
    "husky:uninstall": "npx rimraf .husky && git config --unset core.hooksPath",
    "husky:install": "npx husky install",
    "husky:config:pre-commit": "npx husky add .husky/pre-commit \"npx lint-staged\"",
    "husky": "node scripts/husky-setup.js",
    "prepare": "npx is-ci || npm run husky",
    "prettify": "npx prettier --write .",
    "check:lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "check:typescript": "npx tsc --noEmit",
    "check": "npx run-s prettify check:lint check:typescript",
    "next:dev": "npx next dev",
    "next:export": "npx next export",
    "next:build": "npx next build",
    "next:start": "npx next start",
    "post:next:export": "node scripts/post-export.js",
    "start": "npx run-s next:dev",
    "clean:node_modules-cache": "npx rimraf node_modules/.cache",
    "clean:next-cache": "npx rimraf .next",
    "clean:next-output": "npx rimraf out",
    "clean": "npx run-p clean:node_modules-cache clean:next-cache clean:next-output",
    "build:ci": "npx run-s clean next:build next:export post:next:export",
    "build": "npx run-s clean next:build",
    "serve-nextjs-output": "npx serve out -l 3000",
    "serve": "npx run-s build next:start",
    "serve:ci": "npx cross-env NODE_ENV=local npx run-s build:ci serve-nextjs-output",
    "predeploy:gh-pages": "touch out/.nojekyll",
    "deploy:gh-pages": "npx gh-pages -d out --dotfiles",
    "deploy": "npx run-s build:ci deploy:gh-pages",
    "ci": "npx cross-env NODE_ENV=production npx run-s build:ci",
    "pullrequest": "npx run-s check build",
    "bundle-analyze": "npx cross-env ANALYZE=true npx run-s clean next:build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,md,html,css,scss}": "npx prettier --write",
    "src/**/*.{js,jsx,ts,tsx}": "npx eslint --fix"
  },
  "dependencies": {
    "clsx": "1.1.1",
    "i18next": "21.2.6",
    "i18next-browser-languagedetector": "6.1.2",
    "next": "12.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.12.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.1",
    "@next/eslint-plugin-next": "12.0.1",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "autoprefixer": "^10.4.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "is-ci": "3.0.0",
    "lint-staged": "12.1.2",
    "npm-run-all2": "5.0.2",
    "postcss": "^8.4.4",
    "postcss-import": "14.0.2",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "sass": "1.43.3",
    "serve": "13.0.2",
    "tailwindcss": "3.0.0",
    "typescript": "4.4.4"
  }
}
